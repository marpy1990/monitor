<link rel="stylesheet" href="/static/css/overview/detail.css">
<div id="detail" class="anchor">
	<div class="panel panel-default">
	  <div class="panel-heading">
	    <h3 class="panel-title">基本信息</h3>
	  </div>
	  <div class="panel-body">
	    <table class="table table-hover">
          <thead>
    	    <tr><th>属性</th><th>值</th>
          </thead>
          <tbody>
          	#foreach($elem in $!state)
          		<tr>
          			<td>$elem.key</td>
          			<td>$elem.value</td>
          		</tr>
          	#end
		  </tbody>
    	</table>
  	  </div>
    </div>
</div>